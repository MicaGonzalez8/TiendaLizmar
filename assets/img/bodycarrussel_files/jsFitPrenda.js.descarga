<script>
  $('.mobile-sidebar-nav__list-item').on("click", function(){
    var getCollapseId = $(this).attr("href");
    var collapseId = getCollapseId.slice(-1);
    $(`.sub-list-collapse-${collapseId}`).appendTo($('.sidebar-img-container'))
    if($(`#sub-list-collapse-${collapseId}`).attr("aria-expanded") == "true"){
      $(`.sub-list-collapse-${collapseId}`).hide()
    } else{
      $(`.sub-list-collapse-${collapseId}`).show()
    }
  });
  
  document.addEventListener('DOMContentLoaded', function () {
    const cyberText = document.querySelector('.animation-special-cyber-black-mobile');
    const linkElement = document.querySelector('.main-navigation__item a');
    if (cyberText && linkElement) {
      const textContent = cyberText.textContent.trim();
      cyberText.innerHTML = '';
      textContent.split('').forEach((char) => {
        if (char !== ' ') {
          const span = document.createElement('span');
          span.textContent = char;
          cyberText.appendChild(span);
        }
      });
      const spans = cyberText.querySelectorAll('span');
      let currentIndex = 0;
      const speed = 200;
      spans.forEach(span => {
        span.style.color = 'red';
      });
      function animateLetters() {
        if (currentIndex > 0) {
          spans[currentIndex - 1].style.color = 'red';
        } else {
          spans[spans.length - 1].style.color = 'red';
        }
        spans[currentIndex].style.color = "black"; 
        currentIndex++;
        if (currentIndex >= spans.length) {
          currentIndex = 0; 
        }
        setTimeout(animateLetters, speed);
      }
  
      animateLetters();
    }
  });
</script>
